{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "tags": {
      "value": {
        "availability-level": "a1",
        "environment": "prod",
        "mission": "administrative",
        "po-number": "zzz",
        "protection-level": "p1",
        "source": "micro hack",
        "join-type": "AADDS, ADDS or AAD joined"
      }
    },
    "vnetAddressPrefix": {
      "value": "10.11.0.0/16"
    },
    "snetAddressPrefix": {
      "value": "10.11.0.0/24"
    },
    "aadJoin": {
      "value": true
    },
    "hubVnetName": {
      "value": "<type name of your existing hub vnet from where your DC can be reached>"
    },
    "hubVnetRgName": {
      "value": "<type name of the resource group of your hub vnet>"
    },
    "dnsServer": {
      "value": "<type IP Adress of your custom DNS server>"
    }
  }
}
